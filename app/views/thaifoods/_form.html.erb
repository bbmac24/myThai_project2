<%= form_for @thaifood do |f| %>
  <div class='form-group'>
    Name
    <%= f.text_field :dish, class: 'form-control' %>
  </div>
  <div class='form-group'>
    Location: 
    <%= f.text_field :location, class: 'form-control' %>
  </div>
  <div class='form-group'>
    Restaurant: 
    <%= f.text_field :restaurant, class: 'form-control' %>
  </div>
  <div class='form-group'>
    Unit Price $
    <%= f.number_field :price_per_dish, :step => 0.01, class: 'form-control' %>
  </div>
  <div class='form-group'>
    <%= f.file_field :photo %>

  <div class='form-group'>
    <%= f.submit 'Add Your ThaiDish!', class: 'form-control btn btn-primary' %>
  </div>
<% end %>